{
	"_version":"1.21.0",
	"sap.app": {
		"id": "ToDoMVC",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "ToDoMVC.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.79.2",
			"libs": {
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ToDoMVC.i18n.i18n"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "ToDoMVC.view",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"routes": [{
				"pattern": "",
				"name": "appHome",
				"target": "home"
			}, {
				"pattern": "{filter}",
				"name": "appHomeFilter",
				"target": "home"
				}],
			"targets": {
				"home": {
					"viewName": "Home",
					"viewLevel": 1
				}
			}
		}
	}
}
