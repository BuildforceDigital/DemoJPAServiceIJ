{
  "_version": "1.21.0",
  "sap.app": {
		"applicationVersion": {"version": "1.0.0"},
    "dataSources": {
      "invoiceRemote": {
        "uri": "/ContextPath/ServletPath.svc/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    },
    "description": "{{appDescription}}",
    "i18n": {
      "bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [""],
      "fallbackLocale": ""
    },
    "id": "sap.ui.demo.walkthrough",
    "title": "{{appTitle}}",
    "type": "application"
  },
  "sap.ui": {
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "technology": "UI5"
  },
  "sap.ui5": {
    "rootView": {
      "async": true,
      "type": "XML",
      "viewName": "sap.ui.demo.walkthrough.view.App"
    },
    "dependencies": {
      "minUI5Version": "1.79.2",
      "libs": {
        "sap.m": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "invoice": {
        "dataSource": "invoiceRemote",
        "preload": true,
        "settings": {
          "autoExpandSelect": true,
          "groupId": "$auto",
          "operationMode": "Server",
          "synchronizationMode": "None"
        }
      },
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sap.ui.demo.walkthrough.i18n.i18n",
          "supportedLocales": [""],
          "fallbackLocale": ""
        }
      }
    },
    "resources": {
			"css": [{"uri": "css/style.css"}]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "sap.ui.demo.walkthrough.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "overview",
          "target": "overview"
        },
        {
          "pattern": "detail/{invoicePath}",
          "name": "detail",
          "target": "detail"
        }
      ],
      "targets": {
        "overview": {
          "viewId": "overview",
          "viewName": "Overview"
        },
        "detail": {
          "viewId": "detail",
          "viewName": "Detail"
        }
      }
    }
  }
}