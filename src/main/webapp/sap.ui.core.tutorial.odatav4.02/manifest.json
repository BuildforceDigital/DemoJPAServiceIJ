{
	"_version": "1.25.0",
	"sap.app": {
		"applicationVersion": {"version": "1.0.0"},
		"dataSources": {
			"default": {
				"uri": "/ContextPath/ServletPath.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		},
		"description": "{{appDescription}}",
		"i18n": {"bundleUrl": "i18n/i18n.properties", "supportedLocales": [""], "fallbackLocale": ""},
		"title": "{{appTitle}}",
		"type": "application"
	},
	"sap.ui": {
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"technology": "UI5"
	},
	"sap.ui5": {
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"dependencies": {
			"minUI5Version": "1.79.2",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			}
		},
		"handleValidation": true,
		"models": {
				"": {
					"dataSource": "default",
					"settings": {
						"autoExpandSelect": true,
						"operationMode": "Server",
						"groupId": "$direct",
						"synchronizationMode": "None"
					}
				},
				"i18n": {"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.core.tutorial.odatav4.i18n.i18n",
					"supportedLocales": [""],
					"fallbackLocale": ""
				}
			}
		},

		"rootView": {
			"async": true,
			"viewName": "sap.ui.core.tutorial.odatav4.view.App",
			"type": "XML",
			"id": "appView"
		}
	}
}