{
	"_version": "1.21.0",
	"sap.app": {
		"id": "sap.ui.core.tutorial.odatav4",
		"type": "application",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [
				""
			],
			"fallbackLocale": ""
		},
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"dataSources": {
			"default": {
				"uri": "https://cors-anywhere.herokuapp.com/https://services.odata.org/TripPinRESTierService/(S(id))/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		}
	},
	"sap.ui": {
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"technology": "UI5"
	},
	"sap.ui5": {
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"dependencies": {
			"minUI5Version": "1.48.0",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			}
		},
		"handleValidation": true,
		"models": {
				"": {
					"dataSource": "default",
					"settings": {
						"autoExpandSelect": true,
						"operationMode": "Server",
						"groupId": "$direct",
						"synchronizationMode": "None"
					}
				},
				"i18n": {"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.core.tutorial.odatav4.i18n.i18n",
					"supportedLocales": [
						""
					],
					"fallbackLocale": ""
				}
			}
		},

		"rootView": {
			"viewName": "sap.ui.core.tutorial.odatav4.view.App",
			"type": "XML",
			"async": true,
			"id": "appView"
		}
	}
}